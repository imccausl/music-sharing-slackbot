{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["prettier"],
  "rules": {
    "comma-dangle": [
      "error",
      {
        "functions": "ignore",
        "objects": "only-multiline",
        "arrays": "only-multiline"
      }
    ]
  }
}
